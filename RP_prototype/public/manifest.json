{
  "manifest_version": 3,
  "name": "RP REE Prototype",
  "version": "1.0",
  "description": "Code for Reddit browser extension for RP 598 OM Group 1.",
  "host_permissions": [
    "https://www.reddit.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.reddit.com/*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "script.css"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "script.css",
        "data/rp-logo.png",
        "react-ui/*"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "icons": {
    "128": "data/rp-logo.png" 
  }
}